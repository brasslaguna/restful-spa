    "babel-plugin-transform-export-extensions": "^6.22.0",
    "crypto": "^1.0.1",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.4",
    "sass-loader": "^7.0.1",
    "webpack": "^4.17.0",
    "webpack-cli": "^3.3.1"
  }
}

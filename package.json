{
  "name": "restful-spa",
  "version": "1.0.0",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "start": "concurrently \"bnr start-prod-api\" \"bnr start-prod-app\"",
    "start-dev": "concurrently \"bnr start-dev-api\" \"bnr start-dev-app\"",
    "webpack-watch": "bnr webpack-watch",
    "webpack-build": "bnr webpack-build",
    "create-new-db": "node scripts/createNewDb.js",
    "create-babelrc": "node scripts/createBabelRc.js"
  },
  "betterScripts": {
    "start-prod-app": {
      "command": "node app/server.js",
      "env": {
        "PORT": "3337",
        "API_PORT": "3338",
        "NODE_ENV": "production",
        "BABEL_ENV": "server"
      }
    },
    "start-prod-api": {
      "command": "babel-node api/src/server.js",
      "env": {
        "PORT": "3338",
        "MAX_DB_POOL": 5,
        "NODE_ENV": "production",
        "BABEL_ENV": "server"
      }
    },
    "start-dev-app": {
      "command": "nodemon app/server.js",
      "env": {
        "PORT": "3337",
        "API_PORT": "3338",
        "NODE_ENV": "development",
        "BABEL_ENV": "server"
      }
    },
    "start-dev-api": {
      "command": "babel-node api/src/server.js",
      "env": {
        "PORT": "3338",
        "MAX_DB_POOL": 5,
        "NODE_ENV": "development",
        "BABEL_ENV": "server"
      }
    },
    "webpack-watch": {
      "command": "webpack --mode=development -w --config webpack.config.js"
    },
    "webpack-build": {
      "command": "webpack --mode=production -p --config webpack.config.js"
    }
  },
  "dependencies": {
    "@feathersjs/express": "^1.2.2",
    "@feathersjs/feathers": "3.1.5",
    "@feathersjs/rest-client": "1.3.4",
    "axios": "^0.17.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-css-modules-transform": "^1.6.1",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-html-import-to-string": "0.0.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-remove-strict-mode": "0.0.2",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.26.0",
    "better-npm-run": "^0.1.0",
    "compression": "^1.7.4",
    "concurrently": "^4.1.2",
    "debug": "2.6.8",
    "exenv": "^1.2.2",
    "express": "^4.17.3",
    "express-sslify": "^1.2.0",
    "feathers-hooks-common": "^4.11.1",
    "feathers-sequelize": "^3.1.0",
    "fs-extra": "^8.1.0",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "query-string": "^6.8.3",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.0.3",
    "react-redux": "^5.0.6",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "redial": "^0.5.0",
    "redux": "^3.6.0",
    "redux-auth-wrapper": "^2.0.2",
    "redux-form": "^8.2.6",
    "redux-thunk": "^2.2.0",
    "sequelize": "^4.37.6",
    "sequelize-views-support": "^1.2.2",
    "style-loader": "^1.0.0"
  },
  "devDependencies": {
